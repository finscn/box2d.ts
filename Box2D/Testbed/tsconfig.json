{
  "compilerOptions": {
    "target": "ES2016",
    "module": "system",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "inlineSourceMap": true,
    "baseUrl": ".",
    "paths": {
      "Box2D": [ "../Box2D/Box2D" ],
      "Box2D/*": [ "../Box2D/*" ],
      "Contributions": [ "../Contributions" ],
      "Contributions/*": [ "../Contributions/*" ],
      "Testbed": [ "./Testbed" ],
      "Testbed/*": [ "./Testbed/*" ],
    }
  },
  "include": [
    "**/*.ts"
  ]
}
