{
  "compilerOptions": {
    "target": "ES2016",
    "module": "system",
    "strict": true,
    "strictNullChecks": false,
    "inlineSourceMap": false,
    "preserveConstEnums": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "removeComments": true,
    "sourceMap": true,
    "inlineSources": false,
    "declaration": true,
    "outFile": "../../dist/box2d-ts.js"
  },
  "include": [
    "**/*.ts"
  ]
}
