{
  "compilerOptions": {
    "target": "ES2016",
    "module": "system",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "inlineSourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "Box2D": [ "../Box2D/Box2D" ],
      "Box2D/*": [ "../Box2D/*" ],
      "Testbed": [ "./Testbed" ],
      "Testbed/*": [ "./*" ]
    }
  },
  "include": [
    "**/*.ts"
  ]
}
